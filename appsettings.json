{
    "FUNCTIONS_WORKER_RUNTIME": "python",
    "PYTHON_ISOLATED_WORKER_MODE": "1",
    "WEBSITE_RUN_FROM_PACKAGE": "0",

    "STORAGE_ACCOUNT_NAME": "[YOUR STORAGE ACCOUNT NAME]",
    "CONTAINER_NAME": "[YOUR CONTENT CONTAINER NAME]",
    "LOG_CONTAINER_NAME": "[YOUR LOGS CONTAINER NAME]",

    "BASE_URLS": "https://www.[yourdomain].com;https://www.[anotherdomain].com",
    "ALLOW_DOMAINS": "www.[somesubdomain].com;[somesubdomain].com",
    "SITEMAP_URL": "",

    "CRAWL_CRON": "0 0 2 * * *",

    "MAX_DEPTH": "5",
    "REQUEST_DELAY": "0.5",

    "USER_AGENT": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/124.0.0.0 Safari/537.36",
    "NETWORK_IDLE_WAIT_MS": "5000",

    "SKIP_REGEXES": "\\.(?:css|js)(?:[?#].*)?$;\\.(?:jpe?g|png|gif|svg|webp|bmp|ico)(?:[?#].*)?$;\\.(?:woff2?|ttf|eot)(?:[?#].*)?$;\\.(?:mp3|mp4|avi|mov|wmv)(?:[?#].*)?$;\\.(?:zip|rar|7z|tar\\.gz)(?:[?#].*)?$",

    "SAVE_404": "false",
    "RESPECT_ROBOTS": "false",
    "INCLUDE_PDFS": "true",
    "INCLUDE_DOCX": "true",
    "INCLUDE_XLSX": "true",

    "PAGE_TIMEOUT_MS": 100000,
    "PDF_DOWNLOAD_TIMEOUT_MS": 210000,
    "MAX_PDF_BYTES": 5000000,
    "MAX_CONTENT_CHARS": 200000,

    "AZURE_OPENAI_API_KEY": "[YOUR OPENAI KEY]",
    "AZURE_OPENAI_ENDPOINT": "https://[OPENAI SERVICE NAME].openai.azure.com",
    "AZURE_OPENAI_EMBEDDING_DEPLOYMENT_NAME": "[THE NAME YOU GAVE YOUR MODEL (ie. myproject-text-embedding-3-small)]",
    "AZURE_OPENAI_EMBEDDING_MODEL_NAME": "[THE MODEL NAME (ie. text-embedding-3-small)]",
    "AZURE_OPENAI_API_VERSION": "[VERION OF YOUR MODEL (ie. '2024-12-01-preview' for text-embedding-3-small)]",
    "EMBEDDING_TOKEN_LIMIT": "[TOKEN LIMIT FOR YOUR MODEL (ie. '8191' for text-embedding-3-small & text-ebedding-ada-002)]",
    "TOKEN_OVERLAP": "300",

    "RETRY_COUNT": "3",
    "RETRY_BACKOFF_FACTOR": "2",
    "MAX_WORKERS": "4"
}
